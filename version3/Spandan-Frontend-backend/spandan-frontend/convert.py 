from openpyxl import load_workbook
import json


book = load_workbook(filename="Students.xlsx")
sh = book.active

l = []
for i in range(1, 2000):
	Roll_num = sh.cell(row = i,column =1).value
	email = sh.cell(row = i,column = 2).value


	name = sh.cell(row = i,column = 3).value
	#lastname = sh.cell(row = i,column = 4).value

	
	dic = {"rollNum" : Roll_num,"name":name} 



	l.append(dic)




json_object = json.dumps(l)
with open("IIITBSTudents.json", "w") as outfile:
    outfile.write(json_object)






